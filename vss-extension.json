{
    "manifestVersion": 1,
    "id": "jacked",
    "name": "Jacked",
    "version": "1.0.0",
    "publisher": "Carbonetes",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Vulnerability Scanner",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Container",
        "Security",
        "Scan",
        "Docker",
        "Image",
        "Utility Task"
    ],
    "icons": {
        "default": "images/logo.png"
    },
    "files": [
        {
            "path": "jackedTask"
        }
    ],
    "content": {
        "details": {
          "path": "overview.md"
        },
        "license": {
          "path": "LICENSE"
        }
    },
    "contributions": [
        {
            "id": "jacked",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "jackedTask",
                "iconAsset": "Jacked.jacked/images/logo.png",
                "includesData": {
                    "assets": [
                        "Jacked.jacked/images/logo.png"
                    ]
                }
            }
        }
    ],
    "public" : true
}